<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.tb"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Mobile Surfer"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.user.tb.Home"
            android:label="@string/title_activity_home" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.user.tb.BookingActivity"
            android:label="@string/title_activity_booking" >
        </activity>
        <activity
            android:name="com.example.user.tb.DetailBookingActivity"
            android:label="@string/title_activity_detail_booking" >
        </activity>
        <activity
            android:name="com.example.user.tb.RoomsActivity"
            android:label="@string/title_activity_rooms" >
        </activity>
        <activity
            android:name="com.example.user.tb.LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name="com.example.user.tb.RegisterActivity"
            android:label="@string/title_activity_register" >
        </activity>
    </application>

</manifest>